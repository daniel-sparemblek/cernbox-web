{
  "server": "https://cbox-ocisdev-javier-live.cern.ch",
  "theme": "https://cbox-ocisdev-javier-live.cern.ch/themes/owncloud/theme.json",
  "openIdConnect": {
    "metadata_url": "https://auth.cern.ch/auth/realms/cern/.well-known/openid-configuration",
    "authority": "https://auth.cern.ch/auth/realms/cern/",
    "client_id": "cernbox-service-dev",
    "response_type": "code",
    "scope": "openid profile email"
  },
  "apps": ["files", "text-editor", "pdf-viewer", "search", "external", "admin-settings"],
  "options": {
    "previewFileMimeTypes": [
      "image/gif",
      "image/png",
      "image/jpeg",
      "text/plain",
      "image/tiff",
      "image/bmp",
      "image/x-ms-bmp"
    ]
  },
  "external_apps": [
    {
      "id": "preview",
      "path": "web-app-preview",
      "config": {
        "mimeTypes": ["image/tiff", "image/bmp", "image/x-ms-bmp"]
      }
    }
  ]
}
